/*network_list.html*/

#js-list {
	width: 100%;
	margin-top: 6.5px;
}
.net-list {
	padding: 6.5px 13px;
}
.net-list .row {
	background-color: #FFFFFF;
	padding: 9.5px 0;
	border-radius: 5px;
}
.list-left img {
	display: inline-block;
	width: 100px;
	height: 100px;
	border-radius: 5px;
}
.list-left {
	padding: 0 13px;
}
.list-center {
	padding: 0 0 0 6px;
}
.list-right {
	padding: 0 13px 0 0;
	height: 100px;
	display: table;
}
.list-right div {
	display: table-cell;
	vertical-align: middle;
	text-align: center;
}
.shop-name {
	font-size: 15px;
	color: #333333;
	margin-top: 5px;
}
.shop-address {
	font-size: 12px;
	color: #606060;
	margin: 15px 0;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap; 
}
.list-right img {
	width: 40px;
}
.list-right p {
	width: 100%;
	margin-top: 5px;
	font-size: 12px;
	color: #74DA80;
	text-align: center;
}
.usable {
	display: inline-block;
	line-height: 23px;
	width: 70px;
	text-align: center;
	border-radius: 10px;
	font-size: 15px;
	color: #FFFFFE;
	background-color: #74DA80;
	margin-right: 10px;
}
.totale-num {
	font-size: 18px;
}


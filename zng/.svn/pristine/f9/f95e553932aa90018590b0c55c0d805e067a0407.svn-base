	width: 98.8px;
	height: 72px;
	margin: 0 auto;
}
.shopHeadImg {
	position: absolute;
	left: 0;
	right: 0;
	top: -32.5px;
	display: block;
	width: 65px;
	height: 65px;
	margin: 0 auto;
	border-radius: 50%;
	
}
.borrowBtn {
	width: 51%;
	margin: 0 auto;
	padding: 60px 0 35px 0;
}
.shop-info {
	margin-top: 13px;
	padding: 0 13px;
}
.shop-info-list {
	line-height: 50px;
	font-size: 14px;
	color: #333333;
}
.shop-info-list:nth-child(1) {
	border-bottom: 1px solid #ccc;
}
.shop-info-list img {
	width: 20px;
}
.shop-info-list div:nth-child(1) {
	padding: 0 13px 0 0;
}
.shop-info-list div:nth-child(2),.shop-info-list div:nth-child(3) {
	padding: 0;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.shop-info-list div:nth-child(3) {
	text-align: right;
}

/*progress_bar.html*/

.progress-box{
	width: 100%;
	height: 100%;
	padding: 0;
	display: table;
}
.progress-title{
    font-size: 15px;
    font-weight: 700;
    color: #494949;
    text-align: center;
}
.progress{
	width: 66%;
	height: 1.5%;
	margin: 20px auto 0 auto;
	background: #929292;
	border-radius: 20px;		 		   
	overflow: visible;
}
.progress-center {
	display: table-cell;
	vertical-align: middle;	
}
.progress .progress-bar{
	border-radius: 20px;
	background-color: #74DA80;
	-webkit-animation: animate-positive 5s;
	animation: animate-positive 5s;
	box-shadow: 0 2px 10px #74DA80;
}
@-webkit-keyframes animate-positive{
    0% { width: 0; }
}
@keyframes animate-positive{
    0% { width: 0; }
}
.amap-icon img {
	width: 50px;
}  		